config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://game/util/EventBus.tscn"

[dotnet]

project/assembly_name="SilverAndSteel"

[editor]

script/templates_search_path="res://tools/templates"
naming/default_signal_callback_name="On{node_name}{signal_name}"
naming/default_signal_callback_to_self_name="On{signal_name}"
